[32m+[m[32m                $.ajax({[m
[32m+[m[32m                    url: url,[m
[32m+[m[32m                    type: "GET",[m
[32m+[m[32m                    dataType: "jsonp",[m
[32m+[m[32m                    complete: function(data){[m
